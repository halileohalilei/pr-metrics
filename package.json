{
  "name": "pr-metrics",
  "version": "1.0.0",
  "description": "A Next.js web app to analyze GitHub Pull Request review metrics for teams and individuals",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && cp public/.nojekyll out/.nojekyll",
    "deploy": "npm run export && gh-pages -d out -t",
    "postinstall": "mkdir -p public/mantine && cp node_modules/@mantine/core/styles.css public/mantine/styles.css && cp node_modules/@mantine/core/styles.layer.css public/mantine/styles.layer.css"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mantine/core": "^7.13.5",
    "@mantine/hooks": "^7.13.5",
    "@tabler/icons-react": "^3.35.0",
    "@tanstack/react-query": "^5.90.2",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "next": "14.2.0",
    "postcss": "^8.5.6",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/node": "20.19.19",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "cross-env": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.0",
    "gh-pages": "^6.3.0",
    "typescript": "5.9.3"
  }
}
